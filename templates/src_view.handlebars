<div id="div_src_view">
    <span class="div_src_line_numbers">
    {{#each this}}
        <div class="div_src_line_number" id="src_line_number_{{inc @index}}">{{inc @index}}</div>
    {{/each}}
    </span><span class="div_src_lines">
    {{#each this}}
        <div class="div_src_line" id="src_line_{{inc @index}}">{{#if this}}{{{this}}}{{else}}&nbsp;{{/if}}</div>
    {{/each}}
    </span>
</div>
